{% for article in articles %}
<div class="col-lg-6 col-md-12 article_">
    <div class="container">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">{{ article.titre }}</h5>
                <p class="card-text">{{ article.utitre }}</p>
                <a href="{{ path('article_show', {'id': article.id}) }}" class="btn btn-primary">En savoir plus !</a>
            </div>
            <img class="my-icon" src="{{ asset('build/images/311272.e250b8c2.png') }}">
        </div>
    </div>
</div>
{% endfor %}